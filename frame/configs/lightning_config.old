from .config import config
from dataclasses import dataclass
from hydra.core.config_store import ConfigStore

@dataclass
class ligthningConfig(config):
    everthing_seed: int = 42
    max_epochs: int = 10
    gups: int = 0
    precision: int = 32

cs = ConfigStore.instance()
cs.store(name="ligthning", node=ligthningConfig)